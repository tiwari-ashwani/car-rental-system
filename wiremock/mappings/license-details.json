{
  "request": {
    "method": "POST",
    "urlPath": "/driving-license-api/license/details",
    "headers": {
      "Content-Type": {
        "contains": "application/json"
      }
    },
    "bodyPatterns": [
      { "matchesJsonPath": "$[?(@.licenseNumber == 'DL123456789')]" }
    ]
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "bodyFileName": "license-response.json"
  }
}